{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "TYPEDOC_WATCH=true docusaurus start --port=7000",
    "build": "docusaurus build ",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy && npm run copyapi",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "copyapi": "cp -r ./docs/api ./i18n/en/docusaurus-plugin-content-docs/current/api",
    "postinstall": "cd mint-sdk-js && npm install"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.3",
    "@docusaurus/plugin-ideal-image": "2.4.3",
    "@docusaurus/preset-classic": "2.4.3",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-gfm": "3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.3",
    "@tsconfig/docusaurus": "1.0.7",
    "docusaurus-plugin-typedoc": "0.22.0",
    "prettier": "2.5.1",
    "typedoc": "0.26.10",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "4.9.5"
  }
